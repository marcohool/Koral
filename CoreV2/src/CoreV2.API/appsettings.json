{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "ConnectionString": "YourConnectionStringHere"
  },
  "JWT": {
    "SigningKey": "SigningKeyHere"
  },
  "Cloudinary": {
    "CloudName": "YourCloudNameHere",
    "ApiKey": "YourApiKeyHere",
    "ApiSecret": "YourApiSecretHere"
  },
  "Image": {
    "MaxSize": 10485760,
    "AllowedExtensions": [
      ".jpg",
      ".jpeg",
      ".png"
    ]
  }
}
