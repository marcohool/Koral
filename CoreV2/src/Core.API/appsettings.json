{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "ConnectionString": "[DbConnectionString]"
  },
  "JWT": {
    "Issuer": "https://localhost:5001",
    "Audience": "https://localhost:5001",
    "SigningKey": "SigningKeyHere"
  },
  "Cloudinary": {
    "CloudName": "YourCloudNameHere",
    "ApiKey": "YourApiKeyHere",
    "ApiSecret": "YourApiSecretHere"
  },
  "Image": {
    "MaxSize": 10485760,
    "AllowedExtensions": [
      ".jpg",
      ".jpeg",
      ".png"
    ]
  },
  "KoralMatch": {
    "BaseUri": "[KoralMatchBaseUri]"
  },
  "Matching": {
    "DeltaEThreshold": 60,
    "CosineSimilarityThreshold": 0.5,
    "TopN": 10
  }
}
